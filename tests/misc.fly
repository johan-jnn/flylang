|kind Dividable(
  #(private, readonly)
  fn get_num -> num()

  fn multiple_of(num n, me.num() % n = 0)
)

fn
<T: Dividable>
mystere -> int({T} arr: {},
  each(arr, item, index,
    if(item.multiple_of(3), return @mystere index)
  )
  return -1
)|

cs Animal(
  fn(life,
    me.life: life;
  ),

  fn damage(amount,
    me.life-: amount;
  );

  fn heal(amount,
    me.life+: amount;
  );
);

cs Dog(Animal,
  fn(name,
    Animal(300);
    me.name: name;
  ),

  fn present(
    std.out.push("Hey! I'm &(me.name), with &(me.life)pv.");
  )
);

my_dog: new Dog(3, 5, true);