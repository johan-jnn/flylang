|kind Dividable(
  #(private, readonly)
  fn get_num -> num()

  fn multiple_of(num n, me.num() % n = 0)
)

fn
<T: Dividable>
mystere -> int({T} arr: {},
  each(arr, item, index,
    if(item.multiple_of(3), return @mystere index)
  )
  return -1
)|

fn print_result(f, args,
  std.out("&(f._name)(&(args.join(','))) --> &(f())");
  ()
);

#(print_result)
fn foo(a, b,
  return a + b
);

foo(2, 6);
